<h1>Login<h1>

<form action= "<%= users_url %>" method="post">
    <input type="hidden" name="_method" value="<%= :post %>">

    <label> User Name:
    <input type="text" name="user[username]">
    </label>
    
    <label> Password: 
    <input name="user[password]" type="password">
    </label>

    <input type="submit" value="Login">
</form>